{
  "name": "react-native-sass",
  "version": "0.0.1",
  "description": "Expanding React Native style possibilities",
  "main": "./src/index.js",
  "scripts": {
    "start": "webpack-dev-server --content-base src --hot --inline --client-log-level=none --port 3000",
    "build": "babel src -d plugin_build",
    "dbuild": "npm run build && npm run build",
    "noPlugin": "cp -R babelrcSpare/noPlugins/.babelrc .babelrc",
    "withPlugin": "cp -R babelrcSpare/withPlugins/.babelrc .babelrc",
    "fix": "npm run noPlugin && npm run build && npm run withPlugin && npm run build",
    "clean": "rm -rf plugin_build",
    "lint": "eslint src"
  },
  "engines": {
    "node": ">=4.2.4"
  },
  "author": "Alex Tumanov",
  "license": "WTFPL",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.0",
    "html-loader": "^0.4.5",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-types": "^6.25.0",
    "babylon": "^6.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-native": "^0.46.1"
  }
}
